    "@types/react": "^18.2.61",
    "@types/react-dom": "^18.2.19",
    "@types/react-redux": "^7.1.33",
    "autoprefixer": "^10.4.19",
    "babel-loader": "^9.1.3",
    "css-loader": "^6.10.0",
    "eslint": "^8.57.0",
